{
	"name": "snippets-plus",
	"publisher": "heartacker",
	"displayName": "C/C++ Snippets Plus",
	"description": "Code snippets Plus for C/C++ with OutSide Code (外侧代码) Support",
	"version": "1.0.0",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "c",
				"path": "./snippets/c.json"
			},
			{
				"language": "cpp",
				"path": "./snippets/cpp.json"
			}
		],
		"keybindings": [
			{
				"key": "ctrl+alt+i",
				"mac": "cmd+alt+i",
				"command": "editor.action.insertSnippet",
				"when": "editorTextFocus || editorHasSelection",
				"comment": "insert code snippet, 插入代码片段"
			}
		]
	},
	"galleryBanner": {
		"color": "#E6E0EA",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/heartacker/snippet-dog"
	},
	"icon": "images/cpp.png",
	"license": "Check in License in the root folder"
}